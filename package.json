{
  "name": "dndmailer",
  "version": "1.0.0",
  "description": "API for emailing d&d notifications",
  "main": "app.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "rimraf dist/ && babel ./ --out-dir dist/ --ignore ./node_modules,./.babelrc,./package.json,./npm-debug.log --copy-files",
    "postinstall": "rimraf dist/ && babel ./ --out-dir dist/ --ignore ./node_modules,./.babelrc,./package.json,./npm-debug.log,./img --copy-files",
    "start": "npm run build && node dist/app.js && node dist/MailServer.js",
    "utils": "npm run build && node dist/util/",
    "mail": "npm run build && node dist/MailServer.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kyledavid/dndmailer.git"
  },
  "keywords": [
    "dnd"
  ],
  "author": "KN Webwork",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/kyledavid/dndmailer/issues"
  },
  "homepage": "https://github.com/kyledavid/dndmailer#readme",
  "dependencies": {
    "body-parser": "^1.18.3",
    "dotenv": "^6.1.0",
    "express": "^4.16.4",
    "moment": "^2.22.2",
    "mongoose": "^5.3.9",
    "nodemailer": "^4.6.8"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "rimraf": "^2.6.2"
  }
}
